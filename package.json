{
  "name": "@briohr/nest-broker",
  "version": "1.4.3",
  "description": "NestJS module to produce and consume messages with the broker of your choice. From the BrioHR team.",
  "author": "Benjamin Bellantonio",
  "homepage": "https://github.com/BrioHR/nest-broker",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "tsc --watch",
    "build": "tsc",
    "prepare": "npm run build"
  },
  "keywords": [
    "nestjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.2.1"
  },
  "dependencies": {
    "@nestjs-plus/discovery": "^2.0.2",
    "amqp-connection-manager": "^3.9.0",
    "amqplib": "^0.10.3"
  },
  "devDependencies": {
    "@briohr/eslint-config-briohr-backend": "^1.0.0",
    "@briohr/prettier-config-briohr": "^1.0.0",
    "@nestjs/common": "^9.2.1",
    "@nestjs/core": "^9.2.1",
    "@types/node": "11.13.4",
    "eslint": "^8.29.0",
    "prettier": "^1.17.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.1.0",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prettier": "@briohr/prettier-config-briohr"
}
